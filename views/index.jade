extends layout

block content
  header#intro
    object#jk-logo(type="image/svg+xml", data="images/jk-logo.svg")
      img(src="images/jk-logo-white@3x.png")
    object#jk-bracket(type="image/svg+xml", data="images/bracket.svg")
      img(src="images/bracket-white@3x.png")
    h1#fed
      | front-end developer
      span |
    a#go(href="javascript:void(0);") Go


  nav#navbar.navbar.navbar-default
    .container
      .row       
        .col-xs-3
          .nav-logo
            object#nav-jk(data="images/jk-logo-black.svg", type="image/svg+xml")
              img(src="images/jk-logo-black@1x.png")
            object#nav-bracket(data="images/bracket-black.svg", type="image/svg+xml")
              img(src="images/bracket-black@1x.png")
            h1
              | front-end developer
              span |
        .col-xs-9
          ul
            li 
              a#resume(href="javascript:void(0);") Resume 
            li
              a#contact(href="javascript:void(0);") Contact


  section#about
